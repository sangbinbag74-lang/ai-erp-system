{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",  
      "use": "@vercel/static"
    },
    {
      "src": "backend/manage.py",  
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",  
      "dest": "backend/manage.py"
    },
    {
      "src": "/(.*)",  
      "dest": "/frontend/dist/$1"
    }
  ]
}