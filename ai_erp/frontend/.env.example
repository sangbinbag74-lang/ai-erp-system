# API Configuration
VITE_API_URL=https://your-backend-url.com
VITE_API_VERSION=v1

# Authentication
VITE_AUTH_DOMAIN=your-auth-domain.com

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
